<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="project_form_view_ap" model="ir.ui.view">
			<field name="name">project.form.ap</field>
			<field name="model">project.project</field>
			<field name="inherit_id" ref="project.edit_project" />
			<field name="arch" type="xml">
				<page name="settings" position="after">
					<page name="proposal_list" string="Proposal List">
						<field name="proposal_list_id">
							<tree editable="bottom">
								<field name="crm_a"/>
								<field name="crm_b"/>
								<field name="crm_c"/>
								<field name="crm_d"/>
								<field name="crm_e"/>
								<field name="crm_f"/>
								<field name="crm_g"/>
								<field name="crm_h"/>
								<field name="crm_i"/>
								<field name="crm_j"/>
							</tree>
						</field>
					</page>
				</page>
			</field>

		</record>		
	</data>
</odoo>