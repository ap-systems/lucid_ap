<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<record id="sale_form_view_ap" model="ir.ui.view">
			<field name="name">sale.form.ap</field>
			<field name="model">sale.order</field>
			<field name="inherit_id" ref="sale.view_order_form" />
			<field name="arch" type="xml">
                <field name="payment_term_id" position="after">
                	<field name="proposal_team" widget="many2many_tags"/>
                </field>
            	<field name="sale_order_template_id" position="after">
            		<field name="project_name"/>
            	</field>
				<page name="customer_signature" position="after">
					<page name="proposal_list" string="Proposal List">
						<field name="proposal_list_id">
							<tree editable="bottom">
								<field name="crm_a"/>
								<field name="crm_b"/>
								<field name="crm_c"/>
								<field name="crm_d"/>
								<field name="crm_e"/>
								<field name="crm_f"/>
								<field name="crm_g"/>
								<field name="crm_h"/>
								<field name="crm_i"/>
								<field name="crm_j"/>
							</tree>
						</field>
					</page>
				</page>
			</field>

		</record>		
	</data>
</odoo>